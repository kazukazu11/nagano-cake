<div class="container">
	<div class="row">
		<h1>会員一覧画面</h1>
	</div>
	<table class="table table-hover table-inverse">
        <thead>
          <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% @end_users.each do |end_user| %>
          <tr>
            <td><%= end_user.id%></td>
            <td><%= end_user.last_name%><%= end_user.first_name%></td>
            <td><%= end_user.email%></td>
            <td>
            	<% if end_user.status%>
					有効
				<% else %>
					退会済み
            	<% end %>
            </td>
          </tr>
          <% end %>
        </tbody>
      </table>
</div>